{
  "expo": {
    "name": "Scout Skills",
    "slug": "scout-skills",
    "scheme": "scout-skills",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/imgs/all-skills.png",
    "splash": {
      "image": "./assets/imgs/world-scout-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "/assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.newryscouts.scoutskills",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.newryscouts.scoutskills",
      "versionCode": 1,
      "icon": "./assets/imgs/all-skills.png",
      "googleServicesFile": "./google-services.json"
    },
    "androidStatusBar": {
      "backgroundColor": "#46BDFF",
      "translucent": false,
      "barStyle": "light-content"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/usuzi.ttf"
          ]
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "800971e2-5170-4820-91a6-e1d06889323a"
      }
    }
  }
}
