{
  "expo": {
    "name": "Scout Skills",
    "slug": "scout-skills",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imgs/all-skills.png",
    "splash": {
      "image": "./assets/imgs/world-scout-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "/assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.newryscouts.scoutskills",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "package": "com.newryscouts.scoutskills",
      "versionCode": 1,
      "icon": "./assets/imgs/all-skills.png",
      "googleServicesFile": "./google-services.json"
    },
    "androidStatusBar": {
      "backgroundColor": "#46BDFF",
      "translucent": false,
      "barStyle":"light-content"
    }
  }
}